<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>32_libreria_web_v5</display-name>
  <welcome-file-list>
    <welcome-file>login.html</welcome-file>
  </welcome-file-list>
  <filter>
  	<filter-name>filtroPrueba</filter-name>
  	<filter-class>filter.FiltroPrueba</filter-class>
  </filter>
  <filter>
  	<filter-name>filtroLogin</filter-name>
  	<filter-class>filter.FiltroControlLogin</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>filtroPrueba</filter-name>
  	<url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter-mapping>
  	<filter-name>filtroLogin</filter-name>
  	<url-pattern>/*</url-pattern>
  </filter-mapping>
</web-app>